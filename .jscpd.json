{
  "threshold": 1,
  "reporters": ["console", "json"],
  "ignore": [
    "node_modules/**",
    "dist/**",
    "**/*.d.ts",
    "**/*.config.ts",
    "**/*.config.js",
    ".playwright-mcp/**",
    "storybook-static/**",
    "playwright-report/**",
    "test-results/**",
    "coverage/**",
    "sync-server/node_modules/**",
    "src/auto-imports.d.ts",
    "src/components.d.ts"
  ],
  "format": ["typescript", "javascript", "vue"],
  "minLines": 5,
  "minTokens": 70,
  "output": "./reports/jscpd",
  "exitCode": 1
}
